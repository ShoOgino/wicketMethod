	/**
	 * @param s
	 *            string to encode
	 * @param charsetName
	 *            encoding to use
	 * @return encoded string
	 * @see java.net.URLEncoder#encode(String, String)
	 */
	public String encode(final String s, final Charset charsetName)
	{
		return encode(s, charsetName.name());
	}

