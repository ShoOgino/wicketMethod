	/**
	 * 
	 * @return true if last segment contains a name and not something like "."
	 *         or "..".
	 */
	private boolean isLastSegmentReal()
	{
		if (segments.isEmpty())
		{
			return false;
		}
		String last = segments.get(segments.size() - 1);
		return last.length() > 0 && !".".equals(last) && !"..".equals(last);
	}

