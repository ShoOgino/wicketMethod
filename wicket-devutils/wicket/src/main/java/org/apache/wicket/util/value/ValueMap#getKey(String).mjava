	/**
	 * @see IValueMap#getKey(String)
	 */
	public String getKey(final String key)
	{
		for (Object keyValue : keySet())
		{
			if (keyValue instanceof String)
			{
				String keyString = (String)keyValue;
				if (key.equalsIgnoreCase(keyString))
				{
					return keyString;
				}
			}
		}
		return null;
	}

