	/**
	 * Construct.
	 * 
	 * @param error
	 * @param message
	 */
	public ValidationErrorFeedback(final IValidationError error, final String message)
	{
		if (error == null)
		{
			throw new IllegalArgumentException("Argument [[error]] cannot be null");
		}
		this.error = error;
		this.message = message;
	}

