	/**
	 * Removes behavior from component
	 * 
	 * @param behaviors
	 *            behaviors to remove
	 * 
	 * @return this (to allow method call chaining)
	 */
	public Component remove(final Behavior... behaviors)
	{
		for (Behavior behavior : behaviors)
		{
			Behaviors.remove(this, behavior);
		}
		return this;
	}

