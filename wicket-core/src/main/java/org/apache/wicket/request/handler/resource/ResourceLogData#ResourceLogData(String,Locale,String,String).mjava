	/**
	 * Construct.
	 * 
	 * @param name
	 * @param locale
	 * @param style
	 * @param variation
	 */
	public ResourceLogData(String name, Locale locale, String style, String variation)
	{
		this.name = name;
		this.locale = locale == null ? null : locale.toString();
		this.style = style;
		this.variation = variation;
	}

