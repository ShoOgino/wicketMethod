	/**
	 * @return the url of the external resource
	 */
	public final Url getUrl()
	{
		Url _url = url;

		if (contextRelative)
		{
			String contextRelative = UrlUtils.rewriteToContextRelative(url.toString(), RequestCycle.get());
			_url = Url.parse(contextRelative, url.getCharset());
		}

		return _url;
	}

