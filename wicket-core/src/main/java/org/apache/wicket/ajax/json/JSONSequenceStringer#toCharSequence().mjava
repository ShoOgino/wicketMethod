	/**
	 * @return JSON text as a {@link CharSequence}
	 */
	@Nullable
	public CharSequence toCharSequence()
	{

		if (mode != 'd')
			return null;

		try
		{
			writer.flush();
		}
		catch (IOException e)
		{
			throw new RuntimeException(e);
		}
		return ((StringBufferWriter)writer).getStringBuffer();
	}

