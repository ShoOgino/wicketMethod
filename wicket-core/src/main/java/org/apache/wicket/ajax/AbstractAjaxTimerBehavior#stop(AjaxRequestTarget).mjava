	/**
	 * Stops the timer
	 */
	public final void stop(final AjaxRequestTarget target)
	{
		if (stopped == false)
		{
			stopped = true;

			String timeoutHandle = getTimeoutHandle();

			target.getHeaderResponse().render(
				OnLoadHeaderItem.forScript("clearTimeout(" + timeoutHandle + "); delete "
					+ timeoutHandle + ";"));
		}
	}

