	/**
	 * Pop an array or object scope.
	 * 
	 * @param c
	 *            The scope to close.
	 * @throws JSONException
	 *             If nesting is wrong.
	 */
	private void pop(char c) throws JSONException
	{
		if (top <= 0)
		{
			throw new JSONException("Nesting error.");
		}
		char m = stack[top - 1] == null ? 'a' : 'k';
		if (m != c)
		{
			throw new JSONException("Nesting error.");
		}
		top -= 1;
		mode = top == 0 ? 'd' : stack[top - 1] == null ? 'a' : 'k';
	}

