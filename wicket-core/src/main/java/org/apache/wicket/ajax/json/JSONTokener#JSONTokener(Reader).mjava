	/**
	 * Construct a JSONTokener from a Reader.
	 * 
	 * @param reader
	 *            A reader.
	 */
	public JSONTokener(Reader reader)
	{
		this.reader = reader.markSupported() ? reader : new BufferedReader(reader);
		eof = false;
		usePrevious = false;
		previous = 0;
		index = 0;
		character = 1;
		line = 1;
	}

