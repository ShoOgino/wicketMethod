	private String getShortType(Type t)
	{
		String shortType;
		switch (t)
		{
			case DROP:
				shortType = "d";
				break;
			case ACTIVE:
				shortType = "a";
				break;
			case QUEUE:
			default:
				// 's' comes from 'stack', but it really acts as a queue.
				shortType = "s";
		}
		return shortType;
	}

