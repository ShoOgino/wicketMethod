	/**
	 * @param delegate
	 *            store to delegate to
	 * @param applicationName
	 *            name of application
	 */
	public CryptingPageStore(IPageStore delegate)
	{
		super(delegate);
		try
		{
			random = SecureRandom.getInstance("SHA1PRNG", "SUN");
		}
		catch (GeneralSecurityException ex)
		{
			throw new WicketRuntimeException(ex);
		}
		crypter = newCrypter();
	}

