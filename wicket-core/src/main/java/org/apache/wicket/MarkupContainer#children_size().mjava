	/**
	 * 
	 * @return The size of the children
	 */
	private int children_size()
	{
		if (children == null)
		{
			return 0;
		}
		if (children instanceof Component)
		{
			return 1;
		}
		if (children instanceof List)
		{
			return ((List<?>)children).size();
		}
		return ((Map<?, ?>)children).size();
	}

