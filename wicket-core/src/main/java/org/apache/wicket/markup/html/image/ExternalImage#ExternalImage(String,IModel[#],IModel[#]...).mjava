	/**
	 * Creates an external image
	 * 
	 * @param id
	 *            the component id
	 * @param src
	 *            the model source URL
	 * @param srcSetModels
	 *            a model list of URLs placed in the srcset attribute
	 */
	public ExternalImage(String id, IModel<?> src, IModel<?>... srcSetModels)
	{
		super(id, src);
		this.srcSet = srcSetModels;
	}

