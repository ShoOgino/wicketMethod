	/**
	 * Creates an external image
	 * 
	 * @param id
	 *            the component id
	 * @param srcModel
	 *            the model source URL
	 * @param srcSetModels
	 *            a model list of URLs placed in the srcset attribute
	 */
	public ExternalImage(String id, IModel<?> srcModel, IModel<?>... srcSetModels)
	{
		super(id, srcModel);
		this.srcSetModels = srcSetModels;
	}

