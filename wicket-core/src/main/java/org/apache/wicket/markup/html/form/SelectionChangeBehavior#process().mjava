	/**
	 * Process the form component.
	 */
	private void process()
	{
		formComponent.validate();
		if (formComponent.isValid())
		{
			if (getUpdateModel())
			{
				formComponent.valid();
				formComponent.updateModel();
			}

			onSelectionChanged();
		}
		else
		{
			formComponent.invalid();
		}
		
		onSelectionChanged();
	}

