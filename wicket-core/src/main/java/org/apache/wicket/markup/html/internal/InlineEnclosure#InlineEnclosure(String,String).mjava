	/**
	 * Construct.
	 * 
	 * @param id
	 * @param childId
	 * @param isInlineEnclosure
	 */
	public InlineEnclosure(final String id, final String childId)
	{
		super(id, childId);

		// ensure that the Enclosure is ready for ajax updates
		setOutputMarkupId(true);
		setOutputMarkupPlaceholderTag(true);
		setMarkupId(getId());
	}

