		@Override
		protected void onAfterRender()
		{
			super.onAfterRender();
			if (isRenderChildren())
			{
				// visit every child
				visitItemChildren(this, new IItemCallback()
				{
					@Override
					public void visitItem(TreeItem item)
					{
						item.afterRender();
					}
				});
			}
		}

