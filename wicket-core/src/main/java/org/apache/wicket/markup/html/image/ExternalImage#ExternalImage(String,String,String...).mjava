	/**
	 * Creates an external image
	 * 
	 * @param id
	 *            the component id
	 * @param src
	 *            the source URL
	 * @param srcSet
	 *            a list of URLs placed in the srcset attribute
	 */
	public ExternalImage(String id, String src, String... srcSet)
	{
		this(id, src != null ? Model.of(src) : null, convertToModel(srcSet));
	}

