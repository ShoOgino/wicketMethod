	@Override
	protected void onInitialize()
	{
		// try and make sure the form we need outputs its markup id.
		super.onInitialize();
		CheckGroup<?> group = getGroup();
		if (group != null)
		{
			Form<?> form = group.getForm();
			if (form != null)
			{
				form.setOutputMarkupId(true);
			}
		}
	}

