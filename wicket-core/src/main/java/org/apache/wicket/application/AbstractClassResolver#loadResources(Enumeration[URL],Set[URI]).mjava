	/**
	 * 
	 * @param resources
	 * @param loadedFiles
	 * @throws URISyntaxException
	 *             if URL.toURI() throws
	 */
	private void loadResources(Enumeration<URL> resources, Set<URI> loadedFiles)
		throws URISyntaxException
	{
		if (resources != null)
		{
			while (resources.hasMoreElements())
			{
				final URL url = resources.nextElement();
				URI uri = url.toURI();
				if (!loadedFiles.contains(uri))
				{
					loadedFiles.add(uri);
				}
			}
		}
	}

