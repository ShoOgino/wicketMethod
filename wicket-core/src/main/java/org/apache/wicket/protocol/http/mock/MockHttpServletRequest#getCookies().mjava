	/**
	 * Get all of the cookies for this request.
	 * 
	 * @return The cookies
	 */
	@Override
	public Cookie[] getCookies()
	{
		if (cookies.size() == 0)
		{
			return null;
		}
		Cookie[] result = new Cookie[cookies.size()];
		for (int i = 0; i < cookies.size(); i++)
		{
			result[i] = Cookies.copyOf(cookies.get(i));
		}
		return result;
	}

