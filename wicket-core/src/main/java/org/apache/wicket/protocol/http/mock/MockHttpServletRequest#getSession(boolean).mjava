	/**
	 * Get the session.
	 * 
	 * @param b
	 *            Ignored, there is always a session
	 * @return The session
	 */
	@Override
	public HttpSession getSession(boolean b)
	{
		if (session instanceof MockHttpSession)
		{
			MockHttpSession mockHttpSession = (MockHttpSession)session;
			if (b)
			{
				mockHttpSession.setTemporary(false);
			}
		}
		return session;
	}

