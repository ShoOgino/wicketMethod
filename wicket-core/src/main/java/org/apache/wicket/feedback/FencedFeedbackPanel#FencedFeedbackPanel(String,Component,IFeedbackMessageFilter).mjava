	/**
	 * Creates a fenced feedback panel with a filter.
	 *
	 * @param id
	 * @param fence
	 * @param filter
	 * @see #FencedFeedbackPanel(String, Component)
	 */
	public FencedFeedbackPanel(String id, Component fence, IFeedbackMessageFilter filter)
	{
		super(id, filter);
		this.fence = fence;
		if (fence != null)
		{
			incrementFenceCount();
		}
	}

