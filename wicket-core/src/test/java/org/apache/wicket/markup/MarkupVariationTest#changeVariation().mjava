	@Test
	public void changeVariation()
	{
		tester.startPage(new VariationPage());
		tester.assertContainsNot("Two");
		tester.clickLink("p:l");

		tester.assertContainsNot("One");
		tester.clickLink("p:l");

		tester.assertContainsNot("Two");
		tester.clickLink("p:l");
	}

