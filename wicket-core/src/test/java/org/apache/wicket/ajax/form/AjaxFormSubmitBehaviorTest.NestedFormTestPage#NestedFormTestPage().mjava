		public NestedFormTestPage() 
		{
			Form<?> inner = new Form("inner") {
				@Override
				protected boolean wantSubmitOnParentFormSubmit() {
					return false;
				}
				
				@Override
				protected void onSubmit() {
					super.onSubmit();
					innerSubmitted = true;
				}
			};
			
			inner.add(new AjaxButton("submit", inner) {});
			
			get("outer:inner").replaceWith(inner);
		}

