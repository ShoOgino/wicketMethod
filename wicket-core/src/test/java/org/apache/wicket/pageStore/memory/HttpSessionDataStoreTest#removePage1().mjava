	/**
	 * removePage1()
	 */
	@Test
	public void removePage1()
	{
		assertNull(store.getData(sessionId, pageId));

		store.storeData(sessionId, pageId, PAGE1);

		assertNotNull(store.getData(sessionId, pageId));

		store.removeData(sessionId, pageId);

		assertNull(store.getData(sessionId, pageId));
	}

