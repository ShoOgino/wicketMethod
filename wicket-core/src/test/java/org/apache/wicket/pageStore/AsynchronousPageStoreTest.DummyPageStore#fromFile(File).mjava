		private Object fromFile(File file)
		{
			FileInputStream fis = null;
			ObjectInput oi = null;
			try
			{
				fis = new FileInputStream(file);
				oi = new ObjectInputStream(fis);
				return oi.readObject();
			}
			catch (IOException e)
			{
				throw new RuntimeException(e);
			}
			catch (ClassNotFoundException e)
			{
				throw new RuntimeException(e);
			}
			finally
			{
				try
				{
					if (fis != null)
						fis.close();
					if (oi != null)
						oi.close();
				}
				catch (IOException e)
				{
					throw new RuntimeException(e);
				}
			}
		}

