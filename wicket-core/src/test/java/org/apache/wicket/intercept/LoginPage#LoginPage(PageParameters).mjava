	/**
	 * Construct.
	 * 
	 * @param parameters
	 */
	public LoginPage(final PageParameters parameters)
	{
		super(parameters);

		add(new Form<Void>("form")
		{
			@Override
			public void onSubmit()
			{
				((MySession)getSession()).setAnonymous(false);

				if (!continueToOriginalDestination())
				{
					setResponsePage(SuccessPage.class);
				}
			}
		});
	}

