	/**
	 * Construct.
	 * 
	 * @param id
	 *            the component id
	 */
	public MockPanelWithLink(String id)
	{
		super(id);

		add(new AjaxFallbackLink<Void>("link")
		{
			@Override
			public void onClick(AjaxRequestTarget target)
			{
				MockPanelWithLink.this.onLinkClick(target);
			}
		});
	}

