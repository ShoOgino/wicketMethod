	/**
	 * Print command.
	 * 
	 * @param command
	 *            the command
	 * @return String to command as a plain string
	 */
	private String printCommand(String[] command)
	{
		StringBuffer b = new StringBuffer();
		for (String element : command)
		{
			b.append(element).append(" ");
		}
		return b.toString();
	}

