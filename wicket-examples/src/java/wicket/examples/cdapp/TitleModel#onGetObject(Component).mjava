	/**
	 * @see AbstractDetachableModel#onGetObject(Component)
	 */
	protected Object onGetObject(final Component component)
	{
		if (cdModel.getId() != null) // it is allready persistent
		{
			CD cd = (CD)cdModel.getObject(component);
			return cd.getTitle();
		}
		else // it is a new cd
		{
			return "<NEW CD>";
		}
	}

