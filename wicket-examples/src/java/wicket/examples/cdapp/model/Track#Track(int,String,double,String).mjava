	/**
	 * Construct.
	 * @param number
	 * @param title
	 * @param length
	 * @param performer
	 */
	public Track(int number, String title, double length, String performer)
	{
		super();
		this.number = new Integer(number);
		this.title = title;
		this.length = new Double(length);
		this.performer = performer;
	}

