	/**
	 * Constructor
	 * 
	 * @param parameters
	 *            The page parameters
	 */
	public SignIn2(final PageParameters parameters)
	{
		add(new SignInPanel(this,"signInPanel")
		{
			public boolean signIn(String username, String password)
			{
				return ((SignIn2Session)getSession()).authenticate(username, password);
			}
		});
	}

