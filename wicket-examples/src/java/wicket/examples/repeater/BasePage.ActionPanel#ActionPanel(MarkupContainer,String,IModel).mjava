		/**
		 * @param id
		 *            component id
		 * @param model
		 *            model for contact
		 */
		public ActionPanel(MarkupContainer parent, final String id, IModel model)
		{
			super(parent, id, model);
			add(new Link(this, "select")
			{
				@Override
				public void onClick()
				{
					BasePage.this.selected = (Contact)getParent().getModelObject();
				}
			});
		}

