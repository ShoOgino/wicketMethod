		/**
		 * Constructor
		 * 
		 * @param name
		 *            Name of form
		 */
		public SelectForm(String name)
		{
			super(name, null);
			model = new SelectModel();
			label = new Label("label", new PropertyModel(model, "name"));
			add(label);
			DropDownChoice choice = new DropDownChoice("users", model, new UserIdList());
			add(choice);
		}

