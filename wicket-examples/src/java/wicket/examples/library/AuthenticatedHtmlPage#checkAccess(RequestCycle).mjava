    /**
     * @see wicket.Page#checkAccess(wicket.RequestCycle)
     */
    protected boolean checkAccess(RequestCycle cycle)
    {
        // Is user signed in?
        if (Authenticator.forSession(cycle.getSession()).isSignedIn())
        {
            // okay to proceed
            return true;
        }
        else
        {
            // Force sign in
            cycle.redirectToInterceptPage(SignIn.class);

            return false;
        }
    }

