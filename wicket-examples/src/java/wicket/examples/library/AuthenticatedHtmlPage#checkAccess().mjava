    /**
     * @see wicket.Page#checkAccess()
     */
    protected boolean checkAccess()
    {
        // Is user signed in?
        if (getLibrarySession().isSignedIn())
        {
            // okay to proceed
            return true;
        }
        else
        {
            // Force sign in
            getRequestCycle().redirectToInterceptPage(SignIn.class);
            return false;
        }
    }

