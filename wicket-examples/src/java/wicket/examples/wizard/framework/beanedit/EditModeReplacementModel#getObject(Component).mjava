	/**
	 * @see wicket.model.IModel#getObject(wicket.Component)
	 */
	public Object getObject(Component component)
	{
		if(editMode != null)
		{
			return (editMode == EditMode.READ_ONLY) ? "disabled" : null;
		}
		if(descriptor != null)
		{
			return (descriptor.getWriteMethod() == null) ? "disabled" : null;
		}
		return null;
	}

