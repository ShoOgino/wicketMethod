	/**
	 * internal method that will return null if no tab is selected
	 * 
	 * @return null if no tab is selected or the selected tab
	 */
	private TabPanelModel getSelectedTabModel()
	{
		if (selectedTab < 0)
		{
			return null;
		}
		else
		{
			return (TabPanelModel)tabs.get(selectedTab);
		}
	}

