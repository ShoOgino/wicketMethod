	/**
	 * @see wicket.markup.html.tree.Tree#getNodeImage(javax.swing.tree.DefaultMutableTreeNode)
	 */
	protected Image getNodeImage(final DefaultMutableTreeNode node)
	{
		if (node.isLeaf())
		{
			Image img = new Image(NODE_IMAGE_NAME, "node.gif");
			return img;
		}
		else
		{
			if (isExpanded(node))
			{
				Image img = new Image(NODE_IMAGE_NAME, "folderopen.gif");
				return img;
			}
			else
			{
				Image img = new Image(NODE_IMAGE_NAME, "folder.gif");
				return img;
			}
		}
	}

