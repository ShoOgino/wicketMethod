	/**
	 * @see wicket.markup.html.tree.IndentTree#getNodeImage(javax.swing.tree.DefaultMutableTreeNode)
	 */
	protected AbstractImage getNodeImage(final DefaultMutableTreeNode node)
	{
		if (node.isLeaf())
		{
			return IMG_NODE;
		}
		else
		{
			if (isExpanded(node))
			{
				return IMG_FOLDER_OPEN;
			}
			else
			{
				return IMG_FOLDER;
			}
		}
	}

