	/**
	 * @param parameters
	 * @return
	 */
	private String join(List<org.apache.wicket.util.string.StringValue> values)
	{
	 StringBuilder sb = new StringBuilder();
		for (org.apache.wicket.util.string.StringValue sv : values)
		{
			if (sb.length() > 0)
			{
				sb.append(", ");
			}

			sb.append(sv.toString());
		}
		return sb.toString();
	}

