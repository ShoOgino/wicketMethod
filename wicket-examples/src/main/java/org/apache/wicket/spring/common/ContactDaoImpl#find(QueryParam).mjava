	/**
	 * select contacts and apply sort
	 * 
	 * @param first
	 * @param count
	 * @param sortProperty
	 * @param sortAsc
	 * @return list of contacts
	 */
	public Iterator find(QueryParam qp)
	{
		List sublist = getIndex(qp.getSort(), qp.isSortAsc()).subList(qp.getFirst(),
				qp.getFirst() + qp.getCount());
		return sublist.iterator();
	}

