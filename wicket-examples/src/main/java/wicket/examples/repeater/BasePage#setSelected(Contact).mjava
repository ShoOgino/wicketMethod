	/**
	 * @param selected
	 *            the selected to set
	 */
	public void setSelected(final Contact selected)
	{
		addStateChange(new Change()
		{
			private final Contact old = BasePage.this.selected;

			@Override
			public void undo()
			{
				BasePage.this.selected = old;
			}

		});
		this.selected = selected;
	}

