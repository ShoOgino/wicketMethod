	/**
	 * lookup a media type by key.
	 * 
	 * @param key
	 *            int code
	 * @return MediaTypeEnum or null if no mediaType is found with the given key
	 */
	public static MediaTypeEnum fromCode(final int key)
	{
		for (MediaTypeEnum element : ALL)
		{
			if (key == element.getCode())
			{
				return element;
			}
		}
		// lookup failed
		return null;
	}

