	/**
	 * Creates the TreeTable for the given model and array of columns.
	 * 
	 * @param id
	 *            The component id
	 * @param model
	 *            The tree model
	 * @param columns
	 *            The columns
	 */
	public TreeTable(final String id, final IModel<? extends TreeModel> model,
		final IColumn columns[])
	{
		super(id, model);
		init(columns);
	}

