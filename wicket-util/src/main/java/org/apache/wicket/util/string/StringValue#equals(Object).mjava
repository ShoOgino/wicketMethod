	/**
	 * {@inheritDoc}
	 */
	@Override
	public boolean equals(final Object obj)
	{
		if (obj instanceof StringValue)
		{
			StringValue stringValue = (StringValue)obj;
			return Objects.isEqual(text, stringValue.text) &&
				Objects.isEqual(locale, stringValue.locale);
		}
		else
		{
			return false;
		}
	}

