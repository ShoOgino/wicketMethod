	/**
	 * @see java.util.ArrayList#lastIndexOf(java.lang.Object)
	 */
	@Override
	public int lastIndexOf(final Object elem)
	{
		if (elem == null)
		{
			for (int i = size() - 1; i >= 0; i--)
			{
				if (get(i) == null)
				{
					return i;
				}
			}
		}
		else
		{
			for (int i = size() - 1; i >= 0; i--)
			{
				if (elem == get(i))
				{
					return i;
				}
			}
		}
		return -1;
	}

