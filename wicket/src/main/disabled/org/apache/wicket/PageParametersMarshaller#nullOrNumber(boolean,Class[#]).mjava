	/**
	 * 
	 * @param getter
	 * @param c
	 * @return
	 */
	private static final Object nullOrNumber(final boolean getter, final Class<?> c)
	{
		if (!getter)
		{
			return null;
		}
		if (c.isArray())
		{
			return Array.newInstance(c, 0);
		}
		for (Type t : Type.values())
		{
			if (t.match(c))
			{
				return t.noValue();
			}
		}
		return null;
	}

