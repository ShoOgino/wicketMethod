	/**
	 * Get the session.
	 * 
	 * @param b
	 *            Ignored, there is always a session
	 * @return The session
	 */
	public HttpSession getSession(boolean b)
	{
		if (b && session instanceof MockHttpSession)
		{
			((MockHttpSession)session).setTemporary(false);
		}
		return getSession();
	}

