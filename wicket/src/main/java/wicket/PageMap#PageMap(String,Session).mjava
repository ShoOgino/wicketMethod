	/**
	 * Constructor
	 * 
	 * @param name
	 *            The name of this page map
	 * @param session
	 *            The session holding this page map
	 */
	public PageMap(String name, Session session)
	{
		this.name = name;
		if (session == null)
		{
			throw new IllegalArgumentException("session must be not null");
		}
		this.session = session;
	}

