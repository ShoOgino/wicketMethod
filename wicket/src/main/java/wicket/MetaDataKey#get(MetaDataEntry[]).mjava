	/**
	 * @param metaData
	 *            Array of metadata to search
	 * @return The entry value
	 */
	Serializable get(MetaDataEntry[] metaData)
	{
		if (metaData != null)
		{
			for (MetaDataEntry m : metaData)
			{
				if (equals(m.key))
				{
					return m.object;
				}
			}
		}
		return null;
	}

