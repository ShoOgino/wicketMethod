			/**
			 * Construct.
			 * 
			 * @param formComponent
			 */
			protected MultipleChoiceSelector(FormComponent formComponent)
			{
				super(formComponent);
				if (!allowMultipleChoice(formComponent))
				{
					Assert.fail("Component:'" + formComponent.getPath()
							+ "' Not support multiple selection.");
				}
			}

