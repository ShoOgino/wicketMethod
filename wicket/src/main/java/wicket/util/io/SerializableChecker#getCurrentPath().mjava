	/**
	 * @return name from root to current node concatted with slashes
	 */
	private StringBuffer getCurrentPath()
	{
		StringBuffer b = new StringBuffer();
		for (Iterator it = nameStack.iterator(); it.hasNext();)
		{
			b.append(it.next());
			if (it.hasNext())
			{
				b.append('/');
			}
		}
		return b;
	}

