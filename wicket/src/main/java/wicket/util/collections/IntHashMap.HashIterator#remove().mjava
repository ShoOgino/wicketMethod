		/**
		 * @see java.util.Iterator#remove()
		 */
		public void remove()
		{
			if (current == null)
			{
				throw new IllegalStateException();
			}
			if (modCount != expectedModCount)
			{
				throw new ConcurrentModificationException();
			}
			int k = current.key;
			current = null;
			IntHashMap.this.removeEntryForKey(k);
			expectedModCount = modCount;
		}

