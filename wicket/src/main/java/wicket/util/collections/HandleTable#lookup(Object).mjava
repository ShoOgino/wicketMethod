	/**
	 * Looks up and returns handle associated with given object, or -1 if no
	 * mapping found.
	 * 
	 * @param obj
	 * @return
	 */
	public int lookup(Object obj)
	{
		if (size == 0)
		{
			return -1;
		}
		int index = hash(obj) % spine.length;
		for (int i = spine[index]; i >= 0; i = next[i])
		{
			if (objs[i] == obj)
			{
				return i;
			}
		}
		return -1;
	}

