        /**
         * @see wicket.markup.html.form.validation.DateValidator#onValidate(wicket.markup.html.form.FormComponent, Date)
         */
        public void onValidate(FormComponent formComponent, Date value)
        {
            if(value != null)
            {
                if(value.before(minimum))
                {
                    error(formComponent);
                }
            }
        }

