		/**
		 * Stops this thread.
		 */
		public void stop()
		{
			System.err.println("Total time in saving: " + totalSavingTime);
			System.err.println("Total time in serialization: " + totalSerializationTime);
			synchronized (storePageMap)
			{
				stop = true;
				storePageMap.notifyAll();
			}
		}

