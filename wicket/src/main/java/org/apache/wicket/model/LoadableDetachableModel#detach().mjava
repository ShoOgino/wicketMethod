	/**
	 * @see org.apache.wicket.model.IDetachable#detach()
	 */
	@Override
	public void detach()
	{
		if (attached)
		{
			try
			{
				onDetach();
			}
			finally
			{
				attached = false;
				transientModelObject = null;

				if (log.isDebugEnabled())
				{
					log.debug("removed transient object for " + this + ", requestCycle " +
						RequestCycle.get());
				}
			}
		}
	}

