	/**
	 * Invalidate and remove session store and page manager
	 */
	private void destroy()
	{
		if (sessionStore != null)
		{
			sessionStore.invalidate(RequestCycle.get().getRequest());
			sessionStore = null;
		}

		if (pageManager != null)
		{
			pageManager.destroy();
			pageManager = null;
		}
	}

