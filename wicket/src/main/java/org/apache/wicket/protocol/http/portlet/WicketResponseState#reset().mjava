	/*
	 * (non-Javadoc)
	 * 
	 * @see javax.servlet.ServletResponseWrapper#reset()
	 */
	public void reset()
	{
		resetBuffer(); // fails if committed
		headers = null;
		cookies = null;
		hasStatus = false;
		contentLength = -1;
		if (printWriter == null)
		{
			contentType = null;
			characterEncoding = null;
			locale = null;
		}
	}

