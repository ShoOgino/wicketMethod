	/**
	 * 
	 * @param ex
	 * @param url
	 * @return error text
	 */
	protected String onError(final Exception ex, String url)
	{
		String text = "Invalid URL: " + url + ", exception type: " + ex.getClass().getName() +
			", exception message";
		log.info(text + ": " + ex.getMessage());
		throw new PageExpiredException("Invalid URL", ex);
	}

