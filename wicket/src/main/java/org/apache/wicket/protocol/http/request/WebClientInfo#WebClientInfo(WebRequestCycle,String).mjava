	/**
	 * Construct.
	 * 
	 * @param requestCycle
	 *            the request cycle
	 * @param userAgent
	 *            the user agent
	 */
	public WebClientInfo(WebRequestCycle requestCycle, String userAgent)
	{
		super();
		this.userAgent = userAgent;
		requestCycle.getWebRequest().getHttpServletRequest();
		properties.setRemoteAddress(getRemoteAddr(requestCycle));
		init();
	}

