	/**
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString()
	{
		final AppendingStringBuffer buf = new AppendingStringBuffer(400);
		buf.append(getRootMarkup().getMarkupResourceStream().toString());
		buf.append("\n");

		for (int i = 0; i < size(); i++)
		{
			buf.append(get(i));
		}
		return buf.toString();
	}

