	/**
	 * @see org.apache.wicket.util.value.ValueMap#putAll(java.util.Map)
	 */
	@Override
	public final void putAll(Map map)
	{
		Iterator<?> iter = map.keySet().iterator();
		while (iter.hasNext())
		{
			String key = (String)iter.next();
			checkIdAttribute(key);
		}

		super.putAll(map);
	}

