	/**
	 * @see org.apache.wicket.Component#onRender()
	 */
	@Override
	protected final void onRender()
	{
		if (transformBodyOnly == false)
		{
			execute(new Runnable()
			{
				public void run()
				{
					// Invoke default execution
					AbstractOutputTransformerContainer.super.onRender();
				}
			});
		}
		else
		{
			super.onRender();
		}
	}

