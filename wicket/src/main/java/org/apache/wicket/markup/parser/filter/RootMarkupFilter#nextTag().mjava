	/**
	 * @see org.apache.wicket.markup.parser.IMarkupFilter#nextTag()
	 */
	public final MarkupElement nextTag() throws ParseException
	{
		int type;
		while ((type = next()) != XmlPullParser.TAG)
		{
			if (type == 0)
			{
				return null;
			}
		}

		return parser.getElement();
	}

