	/**
	 * 
	 * @param s
	 * @throws IOException
	 * @throws ClassNotFoundException
	 */
	private void readObject(java.io.ObjectInputStream s) throws IOException, ClassNotFoundException
	{
		if (this instanceof Page)
		{
			((Page)this).readPageObject(s);
		}
		else
		{
			s.defaultReadObject();
		}
	}

