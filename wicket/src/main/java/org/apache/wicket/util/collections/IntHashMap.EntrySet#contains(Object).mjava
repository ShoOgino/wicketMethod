		/**
		 * @see java.util.AbstractCollection#contains(java.lang.Object)
		 */
		@Override
		public boolean contains(Object o)
		{
			if (!(o instanceof Entry))
			{
				return false;
			}
			Entry<V> e = (Entry)o;
			Entry<V> candidate = getEntry(e.getKey());
			return candidate != null && candidate.equals(e);
		}

