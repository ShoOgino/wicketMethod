	/**
	 * Checks if a byte value is whitespace or not.
	 * 
	 * @param byteToCheck
	 *            the byte to check
	 * @return true if byte is whitespace, false otherwise
	 */
	private static boolean isWhiteSpace(byte byteToCheck)
	{
		switch (byteToCheck)
		{
			case ' ' :
			case '\n' :
			case '\r' :
			case '\t' :
				return true;
			default :
				return false;
		}
	}

