	/**
	 * 
	 * @return the page manager
	 */
	public final IPageManager getPageManager()
	{
		if (pageManager == null)
		{
			synchronized (this)
			{
				if (pageManager == null)
				{
					pageManager = pageAccessSynchronizer.adapt(pageManagerProvider.get(getPageManagerContext()));
				}
			}
		}
		return pageManager;
	}

