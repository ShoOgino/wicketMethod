	/** {@inheritDoc} */
	public void respond(RequestCycle requestCycle)
	{
		Page page = reference.getPage();

		// Should page be redirected to?
		if (requestCycle.isRedirect())
		{
			// Redirect to the page
			requestCycle.redirectTo(page);
		}
		else
		{
			// Let page render itself
			page.renderPage();
		}
	}

