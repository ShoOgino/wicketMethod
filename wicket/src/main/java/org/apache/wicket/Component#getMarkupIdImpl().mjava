	/**
	 * Get the markupId
	 * 
	 * @return MarkupId
	 */
	public final Object getMarkupIdImpl()
	{
		String id = getMarkupIdFromMarkup();
		if (id != null)
		{
			return id;
		}

		if (generatedMarkupId != -1)
		{
			return new Integer(generatedMarkupId);
		}

		return getMetaData(MARKUP_ID_KEY);
	}

