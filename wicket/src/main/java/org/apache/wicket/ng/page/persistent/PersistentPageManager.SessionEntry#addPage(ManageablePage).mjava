		/**
		 * Add the page to cached pages if page with same id is not already there
		 * 
		 * @param page
		 */
		private void addPage(ManageablePage page)
		{
			if (page != null)
			{
				for (ManageablePage p : pages)
				{
					if (p.getPageId() == page.getPageId())
					{
						return;
					}
				}
			}
			pages.add(page);
		}

