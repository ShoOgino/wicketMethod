	/**
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
	@Override
	public boolean equals(Object obj)
	{
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		MountParameters other = (MountParameters)obj;
		if (map == null)
		{
			if (other.map != null)
				return false;
		}
		else if (!map.equals(other.map))
			return false;
		return true;
	}

