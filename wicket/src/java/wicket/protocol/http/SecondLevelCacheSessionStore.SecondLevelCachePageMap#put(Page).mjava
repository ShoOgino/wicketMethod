		@Override
		protected void put(Page page)
		{
			if (!page.isStateless())
			{
				String sessionId = getSession().getId();
				if (sessionId != null)
				{
					if (lastPage != page)
					{
						lastPage = page;
						dirty();
					}
					getStore().storePage(sessionId, page);
				}
			}
		}

