    /**
     * Set the cookies.
     * 
     * @param theCookies
     *            The cookies
     */
    public void setCookies(final Cookie[] theCookies)
    {
        cookies.clear();
        for (int i = 0; i < theCookies.length; i++)
        {
            cookies.add(theCookies[i]);
        }
    }

