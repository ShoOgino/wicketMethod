	/**
	 * Construct.
	 * 
	 * @param requestCycle
	 *            the request cycle
	 * @param userAgent
	 *            the user agent
	 */
	public WebClientInfo(WebRequestCycle requestCycle, String userAgent)
	{
		super();

		if (userAgent == null)
		{
			throw new WicketRuntimeException("user agent must be provided");
		}

		this.userAgent = userAgent;

		init();
	}

