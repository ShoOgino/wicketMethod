	/**
	 * @see wicket.settings.IApplicationSettings#setContextPath(java.lang.String)
	 */
	public void setContextPath(String contextPath)
	{
		if(contextPath != null)
		{
			if(!contextPath.startsWith("/") && !contextPath.startsWith("http:"))
			{
				this.contextPath = "/" + contextPath;
			}
			else
			{
				this.contextPath = contextPath;
			}
		}
	}

