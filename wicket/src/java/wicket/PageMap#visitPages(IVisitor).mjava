	/**
	 * @param visitor
	 *            The visitor to call at each Page in this PageMap.
	 */
	final void visitPages(final IVisitor visitor)
	{
	    if (pages != null)
	    {
			for (final Iterator iterator = pages.values().iterator(); iterator.hasNext();)
			{
				visitor.page((Page)iterator.next());
			}
	    }
	}

