	/**
	 * Reattach this component to its parent.
	 * 
	 * @return This
	 */
	public final Component reattach()
	{
		if (getFlag(FLAG_REMOVED_FROM_PARENT) == true)
		{
			if (id.startsWith(AUTO_COMPONENT_PREFIX))
			{
				parent.autoAdd(this);
			}
			else
			{
				parent.add(this);
			}
		}
		return this;
	}

