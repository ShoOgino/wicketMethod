	/**
	 * @param component
	 *            The component that was removed
	 */
	final void componentVisibilityChanged(final Component component)
	{
		if (!component.isAuto())
		{
			checkModification();
			if (getFlag(FLAG_TRACK_CHANGES) && component.isVersioned())
			{
				newVersion();
				versionManager.componentVisibilityChanged(component);
			}
		}
	}

