		/**
		 * Touch this resource, update timer task (if any)
		 *
		 * @param key
		 *            They key of the resource
		 */
		void touch(String key)
		{
			if (idleTask != null)
			{
				idleTask.cancel();
				makeIdleTask(key);
			}
			if (cacheTask != null)
			{
				cacheTask.cancel();
				makeCacheTask(key);
			}
		}

