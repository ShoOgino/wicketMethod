	/**
	 * Set the index of the first listItem to render
	 * @param startIndex First index of model object's list to display
	 * @return This
	 */
	public ListView setStartIndex(final int startIndex)
	{
		this.firstIndex = startIndex;

		if (firstIndex < 0)
		{
			firstIndex = 0;
		}

		return this;
	}

