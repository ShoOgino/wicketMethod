	/**
	 * @see wicket.markup.html.tree.ITreeState#collapseNode(javax.swing.tree.TreeNode)
	 */
	public void collapseNode(TreeNode node)
	{
		if (nodesCollapsed == true)
		{
			nodes.add(node);
		}
		else
		{
			nodes.remove(node);
		}

		for (ITreeStateListener l : listeners)
		{
			l.nodeCollapsed(node);
		}
	}

