	/**
	 * Returns the enumeration as iterable interface.
	 * @param enumeration 
	 * @param <T> 
	 * @return 
	 */
	private final <T> Iterable<T> toIterable(final Enumeration enumeration) 
	{
		return new Iterable<T>() 
		{
			public Iterator<T> iterator() 
			{
				return toIterator(enumeration);
			}
		};
	}

