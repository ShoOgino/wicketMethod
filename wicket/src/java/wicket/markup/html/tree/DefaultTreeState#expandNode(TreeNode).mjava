	/**
	 * @see wicket.markup.html.tree.ITreeState#expandNode(javax.swing.tree.TreeNode)
	 */
	public void expandNode(TreeNode node)
	{
		if (nodesCollapsed == false)
		{
			nodes.add(node);
		}
		else
		{
			nodes.remove(node);
		}

		for (ITreeStateListener l : listeners)
		{
			l.nodeExpanded(node);
		}
	}

