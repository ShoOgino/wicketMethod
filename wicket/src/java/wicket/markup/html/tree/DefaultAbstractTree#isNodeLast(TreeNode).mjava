	/**
	 * Returns whether node is last child of it's parent.
	 * @return
	 */
	private boolean isNodeLast(TreeNode node) {
		TreeNode parent = node.getParent();
		if (parent == null)
		{
			return true;
		}
		else
		{
			return parent.getChildAt(parent.getChildCount() - 1).equals(node);
		}
	}

