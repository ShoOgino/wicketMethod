	/**
	 * Returns whether the given node is visibled, e.g. all it's parents are expanded.
	 */
	private final boolean isNodeVisible(TreeNode node) 
	{
		while (node.getParent() != null) 
		{
			if (isNodeExpanded(node.getParent()) == false)
			{
				return false;
			}
			node = node.getParent();
		}
		return true;
	}

