	/**
	 * Add component to &lt;wicket:head&gt; instead of panel region. 
	 * 
	 * @param child
	 */
	public void addToHeader(final Component child)
	{
	    if (this.headerComponents == null)
	    {
	        this.headerComponents = new ArrayList();
	    }
	    
	    this.headerComponents.add(child);
	}

