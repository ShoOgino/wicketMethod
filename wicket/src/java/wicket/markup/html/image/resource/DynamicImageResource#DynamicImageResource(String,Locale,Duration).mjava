	/**
	 * Creates a dynamic image resource
	 *  
	 * @param format 
	 * 			The format of this resource (like jpg/png) 
	 * @param locale
	 * 			The locale of this resource 
	 * @param idle
	 *          The idle duration timeout
	 * 
	 */
	public DynamicImageResource(String format,Locale locale, Duration idle)
	{
		super(locale,idle);
		this.format = format;
	}

