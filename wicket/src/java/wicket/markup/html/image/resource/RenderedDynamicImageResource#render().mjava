  /**
   * Renders this image
   *
   * @return The image data
   */
  protected byte[] render()
  {
    while (true)
    {
      final BufferedImage image = new BufferedImage(width, height, type);
      if (render((Graphics2D)image.getGraphics()))
      {
        return toImageData(image);
      }
    }
  }

