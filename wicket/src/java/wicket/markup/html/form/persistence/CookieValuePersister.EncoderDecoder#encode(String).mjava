		/**
		 * Encode
		 * 
		 * @param string
		 * @return encoded string
		 */
		private static String encode(final String string)
		{
			try
			{
				return URLEncoder.encode(string, UTF8);
			}
			catch (UnsupportedEncodingException e)
			{
				log.error(e.getMessage(), e);
			}
			return null;
		}

