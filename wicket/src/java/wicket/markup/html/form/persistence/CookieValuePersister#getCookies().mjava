    /**
     * Gets any cookies for request.
     * 
     * @return Any cookies for this request
     */
    private Cookie[] getCookies()
    {
        try
        {
        	// TODO adouma: no Portlet support yet
            return ((ServletWebRequest)getWebRequest()).getHttpServletRequest().getCookies();
        }
        catch (NullPointerException ex)
        {
            // Ignore any app server problem here
        }

        return new Cookie[0];
    }

