		/**
		 * Decode
		 * 
		 * @param string
		 * @return decoded string
		 */
		private static String decode(final String string)
		{
			try
			{
				return URLDecoder.decode(string, UTF8);
			}
			catch (UnsupportedEncodingException e)
			{
				log.error(e.getMessage(), e);
			}
			return null;
		}

