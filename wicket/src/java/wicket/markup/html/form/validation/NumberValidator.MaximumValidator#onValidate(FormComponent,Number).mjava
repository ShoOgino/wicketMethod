		/**
		 * @see wicket.markup.html.form.validation.NumberValidator#onValidate(wicket.markup.html.form.FormComponent,
		 *      Number)
		 */
		@Override
		public void onValidate(FormComponent formComponent, Number value)
		{
			if (value != null)
			{
				if (value.longValue() > maximum)
				{
					error(formComponent);
				}
			}
		}

