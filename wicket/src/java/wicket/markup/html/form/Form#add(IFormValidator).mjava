	/**
	 * Adds a multi-component validator to the form.
	 * 
	 * @see IFormValidator
	 * @param validator
	 *            validator
	 */
	public void add(IFormValidator validator)
	{
		if (validator == null)
		{
			throw new IllegalArgumentException("validator argument cannot be null");
		}
		formValidators_add(validator);
	}

