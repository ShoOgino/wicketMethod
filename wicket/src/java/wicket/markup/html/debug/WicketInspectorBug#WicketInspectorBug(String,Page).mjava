	/**
	 * Constructor
	 * 
	 * @param id
	 *            Component id
	 * @param page
	 *            Page to inspect
	 */
	public WicketInspectorBug(final String id, final Page page)
	{
		super(id);
		Link link = new Link("link")
		{
			private static final long serialVersionUID = 1L;

			public void onClick()
			{
				setResponsePage(new WicketInspector(page));
			}
		};
		link.add(new Image("bug"));
		add(link);
	}

