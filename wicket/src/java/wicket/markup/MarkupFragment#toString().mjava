	/**
	 * @return String representation of markup list
	 */
	@Override
	public final String toString()
	{
		final AppendingStringBuffer buf = new AppendingStringBuffer(400);
		buf.append(this.markup.toString());
		buf.append("\n");

		for (MarkupElement elem : this)
		{
			buf.append(elem);
			buf.append(",");
		}

		return buf.toString();
	}

