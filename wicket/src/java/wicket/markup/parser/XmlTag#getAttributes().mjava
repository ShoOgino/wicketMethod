	/**
	 * Gets a hashmap of this tag's attributes.
	 * 
	 * @return The tag's attributes
	 */
	public AttributeMap getAttributes()
	{
		if (attributes == null)
		{
			if (copyOf == this)
			{
				attributes = new AttributeMap();
			}
			else
			{
				attributes = new AttributeMap(copyOf.attributes);
			}
		}
		return attributes;
	}

