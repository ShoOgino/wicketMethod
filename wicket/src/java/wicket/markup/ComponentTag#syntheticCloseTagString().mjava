	/**
	 * @return A synthetic close tag for this tag
	 */
	public final String syntheticCloseTagString()
	{
		StringBuffer buf = new StringBuffer(30);
		buf.append("</");
		if (getNamespace() != null)
		{
			buf.append(getNamespace()).append(":");
		}
		buf.append(getName()).append(">");

		return buf.toString();
	}

