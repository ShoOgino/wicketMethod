	/**
	 * Reset the markup to its defaults, except for the wicket namespace which
	 * remains unchanged.
	 */
	final void reset()
	{
		this.markup = new MarkupFragments(this);
		this.resource = null;
		this.xmlDeclaration = null;
		this.encoding = null;
		this.headerIndex = NO_HEADER_FOUND;
		this.currentPath = null;
	}

