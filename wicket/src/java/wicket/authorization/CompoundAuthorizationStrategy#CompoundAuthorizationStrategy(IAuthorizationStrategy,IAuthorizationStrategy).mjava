	/**
	 * Convinience constructor that adds two strategies to the chain
	 * 
	 * @param strat1
	 *            authorization strategy
	 * @param strat2
	 *            authorization strategy
	 */
	public CompoundAuthorizationStrategy(IAuthorizationStrategy strat1,
			IAuthorizationStrategy strat2)
	{
		add(strat1);
		add(strat2);
	}

