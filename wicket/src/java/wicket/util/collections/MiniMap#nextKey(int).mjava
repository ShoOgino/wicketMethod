    /**
     * @param start Index to start at
     * @return Index of next non-null key
     */
    private int nextKey(final int start)
    {
        int i = start;

        do
        {
            if (keys[i] != null)
            {
                return i;
            }

            i = nextIndex(i);
        }
        while (i != start);

        return -1;
    }

