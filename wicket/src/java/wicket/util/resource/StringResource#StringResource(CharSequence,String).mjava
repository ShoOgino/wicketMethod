	/**
	 * Construct.
	 * 
	 * @param string
	 *            The resource string
	 * @param contentType
	 *            The mime type of this resource, such as "text/html" or
	 *            "text/xml"
	 */
	public StringResource(final CharSequence string, final String contentType)
	{
		this.string = string;
		this.contentType = contentType;
	}

