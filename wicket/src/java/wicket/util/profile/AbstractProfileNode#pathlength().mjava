    /**
     * @see wicket.util.profile.IObjectProfileNode#pathlength()
     */
    public final int pathlength()
    {
        final IObjectProfileNode[] path = m_path;

        if (path != null)
        {
            return path.length;
        }
        else
        {
            int result = 0;

            for (IObjectProfileNode node = this; node != null; node = node.parent())
            {
                ++result;
            }

            return result;
        }
    }

