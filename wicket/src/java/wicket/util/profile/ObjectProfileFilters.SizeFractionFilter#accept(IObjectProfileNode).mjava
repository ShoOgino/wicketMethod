        /**
         * @see wicket.util.profile.IObjectProfileNode.INodeFilter#accept(wicket.util.profile.IObjectProfileNode)
         */
        public boolean accept(final IObjectProfileNode node)
        {
            if (node.size() >= (m_threshold * node.root().size()))
            {
                return true;
            }
            else
            {
                return false;
            }
        }

