    /**
     * Converts a string array to a string list
     * @param array The array
     * @return The list
     */
    public static StringList valueOf(final String[] array)
    {
        final StringList strings = new StringList(array.length);

        for (int i = 0; i < array.length; i++)
        {
            strings.add(array[i]);
        }

        return strings;
    }

