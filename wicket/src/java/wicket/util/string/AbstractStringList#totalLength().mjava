    /**
     * @return The total length of all Strings in this sequence.
     */
    public int totalLength()
    {
        // Get number of Strings
        final int size = size();

        // Add strings to list
        int totalLength = 0;

        for (int i = 0; i < size; i++)
        {
            totalLength += get(i).length();
        }

        return totalLength;
    }

