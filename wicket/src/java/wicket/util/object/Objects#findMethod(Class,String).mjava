	private final static Method findMethod(Class clz, String expression)
	{
		if(expression.endsWith("()"))
		{
			expression = expression.substring(0,expression.length()-2);
		}
		Method method = null;
		try
		{
			method = clz.getMethod(expression, null);
		}
		catch (Exception ex)
		{
			// TODO log
		}
		return method;
	}

