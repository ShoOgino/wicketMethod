		private final static Method findSetter(Method getMethod, Class clz)
		{
			String name = getMethod.getName();
			name = "set" + name.substring(3);
			try
			{
				return clz.getMethod(name, new Class[] { int.class, getMethod.getReturnType() });
			}
			catch (Exception ex)
			{
				// TODO LOG
			}
			return null;
		}

