    /**
     * Visits the pages in this session.
     * 
     * @param visitor
     *            The visitor to call
     */
    final void visitPages(final IPageVisitor visitor)
    {
        // Loop through pages in page map
        for (final Iterator iterator = pages.values().iterator(); iterator
                .hasNext();)
        {
            // Visit next page
            visitor.page((Page) iterator.next());
        }
    }

