	/**
	 * @see wicket.model.IModel#setObject(java.lang.Object)
	 */
	public final void setObject(final T object)
	{
		attach();
		try
		{
			onSetObject(object);
		}
		catch (RuntimeException e)
		{
			throw new WicketRuntimeException("unable to set object " + object + ", model: " + this, e);
		}
	}

