	/**
	 * Constructor
	 * 
	 * @param modelObject
	 *            The model object
	 */
	public CompoundPropertyModel(final Object modelObject)
	{
		if (modelObject == null)
		{
			throw new IllegalArgumentException("Model parameter must not be null");
		}
		
		this.modelObject = modelObject;
		this.model = null;
	}

