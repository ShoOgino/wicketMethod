    /**
     * Detaches from the current request.
     * @param cycle the current request cycle
     * @see wicket.model.IDetachableModel#detach(wicket.RequestCycle)
     */
    public final void detach(final RequestCycle cycle)
    {
        if (attached)
        {
            doDetach(cycle);
            attached = false;
        }
    }

