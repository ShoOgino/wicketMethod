    /**
     * Attaches to the current request.
     * @param cycle the current request cycle
     * @see wicket.model.IDetachableModel#attach(wicket.RequestCycle)
     */
    public final void attach(final RequestCycle cycle)
    {
        if (attached)
        {
            return;
        }

        doAttach(cycle);
        attached = true;
    }

