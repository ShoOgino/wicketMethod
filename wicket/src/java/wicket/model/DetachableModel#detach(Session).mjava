    /**
     * Detaches from the current session.
     * 
     * @param session
     *            The current session
     * @see wicket.model.IDetachableModel#detach(wicket.Session)
     */
    public final void detach(final Session session)
    {
        if (attached)
        {
            doDetach(session);
            attached = false;
        }
    }

