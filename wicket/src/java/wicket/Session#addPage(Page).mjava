    /**
     * Adds page to session if not already added.
     * 
     * @param page
     *            Page to add to this session
     */
    final void addPage(final Page page)
    {
        // Set session and identifier
        page.setSession(this);
        page.setId(pageId++);

        // Add to page map
        pages.put(new Integer(page.getId()), page);
    }

