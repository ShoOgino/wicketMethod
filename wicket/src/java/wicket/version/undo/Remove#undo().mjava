	/**
	 * @see wicket.version.undo.Change#undo()
	 */
	public void undo()
	{
		if (log.isDebugEnabled())
		{
			log.debug("UNDO REMOVE: re-adding " + component.getPath() + " ("
					+ Classes.name(component.getClass()) + "@" + component.hashCode()
					+ ") to parent");
		}

		container.internalAdd(component);
	}

