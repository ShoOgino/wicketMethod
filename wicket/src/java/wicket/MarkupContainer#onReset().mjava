	/**
	 * @see wicket.Component#onReset()
	 */
	protected void onReset()
	{
		// Reset this container component
		super.onReset();
		
		// Iterate through children on this container
		for (Iterator iterator = iterator(); iterator.hasNext();)
		{
			// Reset child
			((Component)iterator.next()).reset();
		}
	}

