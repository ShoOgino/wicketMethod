	/**
	 * @param component
	 *            The component that was added
	 */
	final void componentAdded(final Component component)
	{
		if (!component.isAuto())
		{
			checkModification();
			if (getFlag(FLAG_TRACK_CHANGES) && component.isVersioned())
			{
				newVersion();
				versionManager.componentAdded(component);
			}
		}
	}

