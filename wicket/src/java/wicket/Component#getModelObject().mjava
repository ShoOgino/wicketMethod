	/**
	 * Gets the backing model object; this is shorthand for
	 * getModel().getObject().
	 * 
	 * @return the backing model object
	 */
	public final Object getModelObject()
	{
		IModel mod = getModel();
		if (mod != null)
		{
			return getModel().getObject();
		}
		else
		{
			return null;
		}
	}

