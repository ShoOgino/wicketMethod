	/**
	 * THIS METHOD IS NOT PART OF THE WICKET PUBLIC API. DO NOT CALL IT.
	 */
	public final void request()
	{
		try
		{
			// Start of request to page
			internalBeginRequest();

			// Handle request by rendering page
			render();
		}
		finally
		{
			// The request is over
			internalEndRequest();
		}
	}

