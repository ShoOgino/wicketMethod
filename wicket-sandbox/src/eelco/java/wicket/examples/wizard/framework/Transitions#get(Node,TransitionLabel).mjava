	/**
	 * Gets a transition.
	 * 
	 * @param node
	 *            the node
	 * @param label
	 *            for the label
	 * @return the found transition or null if not found
	 */
	public Transition get(Node node, TransitionLabel label)
	{
		Map/* <TransitionLabel,Transition> */m = (Map)transitions.get(node);
		if (m != null)
		{
			return (Transition)m.get(label);
		}

		return null;
	}

