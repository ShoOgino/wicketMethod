	/**
	 * Loads value from cookie
	 * 
	 * @param component
	 *            The component
	 */
	public void load(final FormComponent component)
	{
		final String value = getValue(component.getId());
		if (value != null)
		{
			component.setModelValue(value);
		}
		else
		{
			component.setModelValue("");
		}
	}

