	/**
	 * Returns the resource reference for icon of specified tree node.
	 */
	protected ResourceReference getNodeIcon(TreeNode node)
	{
		if (node.isLeaf() == true)
		{
			return getItem();
		}
		else
		{
			if (isNodeExpanded(node))
				return getFolderOpen();
			else
				return getFolderClosed();
		}
	}

