	/**
	 * @see org.apache.wicket.injection.IFieldValueFactory#supportsField(java.lang.reflect.Field)
	 */
	public boolean supportsField(Field field)
	{
		Iterator it = delegates.iterator();
		while (it.hasNext())
		{
			final IFieldValueFactory factory = (IFieldValueFactory)it.next();
			if (factory.supportsField(field))
			{
				return true;
			}
		}
		return false;
	}

