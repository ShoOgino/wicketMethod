	/**
	 * @param key
	 * @param sessionDir
	 * @return The file pointing to the page
	 */
	private File getPageFile(SessionPageKey key, File sessionDir)
	{
		return new File(sessionDir, appName + "-pm-" + key.pageMap + "-p-" + key.id + "-v-"
				+ key.versionNumber + "-a-" + key.ajaxVersionNumber);
	}

