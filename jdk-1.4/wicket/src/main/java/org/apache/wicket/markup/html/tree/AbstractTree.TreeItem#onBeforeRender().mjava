		protected void onBeforeRender()
		{
			AbstractTree.this.onBeforeRenderInternal();
			super.onBeforeRender();
			
			if (isRenderChildren())
			{
				// visit every child
				visitItemChildren(this, new IItemCallback()
				{
					public void visitItem(TreeItem item)
					{
						item.beforeRender();
					}
				});
			}
		}

