	/**
	 * Undoes the effect of {@link #encodePageMapName(String)}
	 * 
	 * @param pageMapName
	 *            page map name
	 * @return decoded page map name
	 */
	public static String decodePageMapName(String pageMapName)
	{
		if (DEFAULT_PAGEMAP_NAME.equals(pageMapName))
		{
			return null;
		}
		else
		{
			return pageMapName;
		}
	}

