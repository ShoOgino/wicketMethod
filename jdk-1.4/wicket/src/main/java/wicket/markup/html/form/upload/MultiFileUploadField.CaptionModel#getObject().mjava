		/**
		 * @see wicket.model.AbstractReadOnlyModel#getObject()
		 */
		public Object getObject()
		{
			if (max == UNLIMITED)
			{
				return getString(RESOURCE_UNLIMITED);
			}
			else
			{
				return getString(RESOURCE_LIMITED, Model.valueOf(Collections.singletonMap("max",
						new Integer(max))));
			}
		}

